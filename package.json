{
  "name": "financemanagerdashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx,json,css,scss,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,json,css,scss,html}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "env:check": "node scripts/check-env.mjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "ci:lint": "npm run lint",
    "ci:typecheck": "npm run typecheck",
    "ci:test": "npm run test",
    "ci:build": "npm run build",
    "ci:verify": "npm run ci:lint && npm run ci:typecheck && npm run ci:test && npm run ci:build",
    "auth:dev": "tsx --watch services/auth/src/server.ts",
    "auth:build": "tsc --project services/auth/tsconfig.json",
    "auth:start": "node services/auth/dist/server.js",
    "auth:test": "vitest run --config services/auth/vitest.config.cjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/sensible": "^5.6.0",
    "@prisma/client": "^5.21.1",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "argon2": "^0.41.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.35.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.19.17",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "cors": "^2.8.5",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^5.21.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
